{
	"name": "sveltekit-supabase-adyen",
	"version": "0.0.1",
	"private": true,
	"publishConfig": {
		"directory": "dist",
		"linkDirectory": true
	},
	"scripts": {
		"supabase:start": "docker compose -f supabase/docker-compose.yml up -d",
		"supabase:stop": "docker compose -f supabase/docker-compose.yml down",
		"dev:start": "pnpm supabase:start",
		"dev:stop": "pnpm supabase:stop",
		"dev": "vite dev --port 3000",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"dependencies": {
		"supabase-auth-helpers-sveltekit": "workspace:*",
		"@supabase/supabase-js": "^1.35.6",
		"@adyen/adyen-web": "^5.23.1",
		"@adyen/api-library": "^12.0.0",
		"country-locale-map": "^1.8.8"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"autoprefixer": "^10.4.7",
		"tailwindcss": "^3.1.5"
	},
	"type": "module"
}
